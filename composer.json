{
    "name": "crocodicstudio/crudbooster",
    "description": "CRUDBooster Is CRUD Generator Based On Laravel Easy To Make An Advanced Backend",
    "type": "library",
    "license": "MIT",
    "keywords":[
        "laravel",
        "admin-panel",
        "backend",
        "crud-generator",
        "crudbooster",
        "crud",
        "generator"
    ],
    "authors": [
        {
            "name": "Ferry",
            "email": "ferry@crocodic.com",
            "homepage": "https://crudbooster.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "laravel/framework":"^6.0|^7.0|^8.0",
        "barryvdh/laravel-dompdf": "0.8.*",
        "doctrine/dbal": "~2.3",
        "maatwebsite/excel": "^3.1",
        "intervention/imagecache":"^2.3",
        "ext-fileinfo": "*",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
          "Crocodic\\CrudBooster\\": "src/"
        },
        "files": [
            "Core/Helpers/General.php"
        ]
      },
    "extra": {
        "laravel": {
            "providers": [
                "Crocodic\\CrudBooster\\CRUDBoosterServiceProvider"
            ]
        }
    }
}
